{
  "name": "backend",
  "version": "1.0.0",
  "description": "Auction backend service with NestJS",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "nest build",
    "build:test": "tspc -p test/tsconfig.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:coverage-check": "jest --coverage --collectCoverage=true && node scripts/check-coverage.js",
    "test:e2e": "pnpm build:test && node dist-test/test/index.js",
    "benchmark": "pnpm build:test && node dist-test/test/benchmark.js",
    "load-test": "pnpm exec ts-node test/load-test.ts",
    "sdk": "pnpm exec nestia sdk",
    "sdk:e2e": "pnpm exec nestia e2e",
    "asyncapi": "pnpm exec ts-node scripts/generate-asyncapi.ts",
    "prepare": "ts-patch install"
  },
  "dependencies": {
    "@fastify/helmet": "^13.0.2",
    "@fastify/static": "^9.0.0",
    "@grammyjs/validator": "^1.0.1",
    "@nestia/core": "^10.0.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^11.1.11",
    "@nmime/nestjs-asyncapi": "2.0.7",
    "@socket.io/redis-adapter": "^8.3.0",
    "bullmq": "^5.66.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "fastify": "^5.6.2",
    "grammy": "^1.39.2",
    "ioredis": "5.9.1",
    "joi": "^18.0.2",
    "js-yaml": "^4.1.0",
    "mongoose": "^9.1.2",
    "nestjs-i18n": "^10.6.0",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.3",
    "typia": "^11.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@asyncapi/generator": "^3.1.0",
    "@eslint/js": "^9.39.2",
    "@nestia/benchmark": "^10.0.1",
    "@nestia/e2e": "^10.0.1",
    "@nestia/fetcher": "^10.0.1",
    "@nestia/sdk": "^10.0.1",
    "@nestjs/cli": "~10.4.8",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@types/ioredis-mock": "^8.2.6",
    "@types/jest": "^30.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "ioredis-mock": "^8.13.1",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^11.0.1",
    "nestia": "^10.0.1",
    "prettier": "^3.7.4",
    "socket.io-client": "^4.8.3",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~5.6.3",
    "typescript-transform-paths": "^3.5.6"
  }
}