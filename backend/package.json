{
  "name": "backend",
  "version": "1.0.0",
  "description": "Auction backend service with NestJS",
  "main": "dist/src/main.js",
  "scripts": {
    "build": "nest build",
    "build:e2e": "tspc -p test/tsconfig.json",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "test": "vitest run",
    "test:unit": "vitest run --config vitest.config.unit.ts",
    "test:integration": "vitest run --config vitest.config.integration.ts",
    "test:watch": "vitest",
    "test:debug": "vitest --inspect-brk --no-file-parallelism",
    "test:ui": "vitest --ui",
    "test:cov": "vitest run --coverage",
    "test:cov:unit": "vitest run --config vitest.config.unit.ts --coverage",
    "test:e2e": "pnpm build:e2e && node dist-test/test/index.js",
    "benchmark": "pnpm build:e2e && node dist-test/test/benchmark.js",
    "load-test": "artillery run test/artillery/load-test.yml",
    "load-test:smoke": "artillery run -e smoke test/artillery/load-test.yml",
    "load-test:stress": "artillery run -e stress test/artillery/load-test.yml",
    "load-test:http-max": "artillery run test/artillery/http-max-throughput.yml",
    "load-test:edge": "artillery run test/artillery/edge-cases.yml",
    "load-test:ws": "artillery run test/artillery/websocket-test.yml",
    "load-test:ws-max": "artillery run test/artillery/websocket-max-throughput.yml",
    "load-test:all": "artillery run test/artillery/load-test.yml && artillery run test/artillery/edge-cases.yml && artillery run test/artillery/websocket-test.yml",
    "load-test:report": "artillery run --output test/artillery/reports/load-test.json test/artillery/load-test.yml && artillery report test/artillery/reports/load-test.json",
    "sdk": "pnpm exec nestia sdk",
    "sdk:e2e": "pnpm exec nestia e2e",
    "asyncapi": "pnpm exec ts-node scripts/generate-asyncapi.ts",
    "prepare": "ts-patch install"
  },
  "dependencies": {
    "@fastify/helmet": "^13.0.2",
    "@fastify/static": "^9.0.0",
    "@grammyjs/validator": "^1.0.1",
    "@nestia/core": "^10.0.1",
    "@nestjs/bullmq": "^11.0.4",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/platform-express": "^11.1.12",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/platform-socket.io": "^11.1.11",
    "@nestjs/schedule": "^6.1.0",
    "@nestjs/swagger": "^11.2.4",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/websockets": "^11.1.11",
    "@nmime/nestjs-asyncapi": "2.0.9",
    "@socket.io/cluster-adapter": "^0.3.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@socket.io/sticky": "^1.0.4",
    "bullmq": "^5.66.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "fastify": "^5.6.2",
    "grammy": "^1.39.2",
    "ioredis": "5.9.1",
    "joi": "^18.0.2",
    "js-yaml": "^4.1.0",
    "mongoose": "^9.1.2",
    "nestjs-i18n": "^10.6.0",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "socket.io": "^4.8.3",
    "typia": "^11.0.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@asyncapi/generator": "^3.1.0",
    "@eslint/js": "^9.39.2",
    "@nestia/benchmark": "^10.0.1",
    "@nestia/e2e": "^10.0.1",
    "@nestia/fetcher": "^10.0.1",
    "@nestia/sdk": "^10.0.1",
    "@nestjs/cli": "~10.4.8",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.11",
    "@ryoppippi/unplugin-typia": "^2.6.5",
    "@swc/core": "^1.15.10",
    "@types/ioredis-mock": "^8.2.6",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "@vitest/coverage-v8": "^4.0.17",
    "artillery": "^2.0.27",
    "artillery-plugin-expect": "^2.7.0",
    "artillery-plugin-metrics-by-endpoint": "^1.2.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "ioredis-mock": "^8.13.1",
    "mongodb-memory-server": "^11.0.1",
    "nestia": "^10.0.1",
    "prettier": "^3.7.4",
    "socket.io-client": "^4.8.3",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.3.0",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.53.0",
    "typescript-transform-paths": "^3.5.6",
    "unplugin-swc": "^1.5.9",
    "vitest": "^4.0.17"
  }
}