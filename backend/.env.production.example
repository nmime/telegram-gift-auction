# Production (docker-compose.yml)
# Copy to .env before building: cp .env.production.example .env

NODE_ENV=production
PORT=3001

# MONGODB_URI and REDIS_URL are set by docker-compose.yml environment

# JWT secret (generate a secure random string, min 32 chars)
JWT_SECRET=your-secure-production-jwt-secret-min-32-chars

# CORS origin (your production domain)
CORS_ORIGIN=https://your-domain.com
